<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="500.0" prefWidth="760.0" style="-fx-background-color: lemonchiffon;" userData="Temperature Stats" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.TemperatureListController">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="82.0" prefWidth="778.0" text="Temperature Stats">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <HBox alignment="CENTER" prefHeight="353.0" prefWidth="600.0">
         <children>
            <TableView fx:id="temperatureTable" prefHeight="354.0" prefWidth="476.0">
              <columns>
                <TableColumn fx:id="nameColumn" prefWidth="179.0" text="Name" />
                <TableColumn fx:id="temperatureColumn" prefWidth="152.5" text="Temperature" />
                  <TableColumn fx:id="timeDateColumn" prefWidth="145.0" text="Time" />
              </columns>
            </TableView>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="77.0" prefWidth="600.0" spacing="120.0">
         <children>
            <Button mnemonicParsing="false" onAction="#onBack" prefHeight="39.0" prefWidth="90.0" style="-fx-background-radius: 25;" text="Back">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
