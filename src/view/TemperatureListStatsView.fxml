<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="500.0" prefWidth="760.0" userData="Temperature Stats" style="-fx-background-color: lemonchiffon;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.ControllerTemperatureList">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="82.0" prefWidth="778.0" text="Temperature Stats">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <HBox alignment="CENTER" prefHeight="353.0" prefWidth="600.0">
         <children>
            <TableView prefHeight="354.0" prefWidth="476.0" fx:id="temperatureTable">
              <columns>
                <TableColumn prefWidth="156.79999387264252" fx:id="nameColumn" text="Name" />
                <TableColumn prefWidth="194.4000244140625" fx:id="temperatureColumn" text="Temperature" />
                  <TableColumn prefWidth="124.800048828125" fx:id="timeDateColumn" text="Time/Date?" />
              </columns>
            </TableView>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="77.0" prefWidth="600.0" spacing="120.0">
         <children>
            <Button mnemonicParsing="false" onAction="#onBack" prefHeight="39.0" prefWidth="90.0" style="-fx-background-radius: 25;" text="Back">
               <font>
                  <Font size="18.0" />
               </font>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
